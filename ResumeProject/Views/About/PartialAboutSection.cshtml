@model List<ResumeProject.Models.TblProfile>

<section class="w3l-content-6" id="skills">
    <div class="content-6-mian py-5">
        <div class="container py-lg-5 py-md-4 py-2">
            <div class="content-info-in row">
                <div class="content-gd col-lg-5 pe-lg-5 my-image">
                    <img src="~/Content/starter/assets/images/about-me.jpg" alt="" class="img-fluid" />
                </div>
                <div class="content-gd col-lg-7 mt-lg-0 mt-5 align-self ps-lg-5">
                    <h6 class="w3l-title-small mb-1">Hakkımda</h6>
                    <h3 class="w3l-title-main mb-lg-4 mb-3">Hakkımda Kısa Bir Bilgi</h3>
                    @foreach (var x in Model)
                    {
                        <p class="my-4 pe-lg-5">@x.profileDescription</p>
                        <h5 class="w3l-subtitle mb-md-5 mb-4">Kullanabildiğim bazı teknolojiler</h5>
                    }
                    @Html.Action("PartialTechs")
                </div>
            </div>
        </div>
    </div>
</section>

