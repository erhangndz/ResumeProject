@model List<ResumeProject.Models.TblTech>

@foreach (var x in Model)
{
    <div class="progress-info mt-5">
        <h6 class="progress-tittle">@x.TechTitle</h6>
        <div class="progress">
            <div class="progress-bar progress-bar-striped" role="progressbar" style="width: @x.TechValue%" aria-valuenow="@x.TechValue" aria-valuemin="0" aria-valuemax="100">
            </div>
        </div>
    </div>
}
